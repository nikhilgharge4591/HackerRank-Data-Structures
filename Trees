
def preOrder(root):
    #Write your code here
    if root: 
        # First print the data of node 
        print(root.info, end =" ")

        # Then recur on left child 
        preOrder(root.left) 
  
        # Finally recur on right child 
        preOrder(root.right)



def postOrder(root):
    #Write your code here
   if root == None:
    return
   postOrder(root.left)
   postOrder(root.right)
   print(root.info,end=" ")
   
   
 
def inOrder(root):
    #Write your code here
    if root == None:
        return 
    inOrder(root.left)
    print(root.info, end = " ")
    inOrder(root.right)


def levelOrder(root):
    #Write your code here
    if root == None:
        return
    queue = [root]
    while len(queue) > 0:
        temp_node = queue.pop(0)
        print(temp_node.info, end = " ")

        if temp_node.left is not None:
            queue.append(temp_node.left)
        
        if temp_node.right is not None:
            queue.append(temp_node.right)
