
def preOrder(root):
    #Write your code here
    if root: 
        # First print the data of node 
        print(root.info, end =" ")

        # Then recur on left child 
        preOrder(root.left) 
  
        # Finally recur on right child 
        preOrder(root.right)



def postOrder(root):
    #Write your code here
   if root == None:
    return
   postOrder(root.left)
   postOrder(root.right)
   print(root.info,end=" ")
   
   
 
def inOrder(root):
    #Write your code here
    if root == None:
        return 
    inOrder(root.left)
    print(root.info, end = " ")
    inOrder(root.right)
